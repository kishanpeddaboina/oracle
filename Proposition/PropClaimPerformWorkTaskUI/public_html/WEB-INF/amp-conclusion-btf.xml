<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="amp-conclusion-btf">
    <default-activity>conclusion.jsff</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__1">
      <name>claimResult</name>
      <value>#{pageFlowScope.claimResult}</value>
      <class>java.lang.String</class>
    </input-parameter-definition>
    <input-parameter-definition id="__2">
      <name>PartialShow</name>
      <value>#{pageFlowScope.PartialShow}</value>
      <class>java.lang.String</class>
    </input-parameter-definition>
    <view id="conclusion.jsff">
      <page>/conclusion.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
