<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="2.2" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
    <locale-config>
      <default-locale>en</default-locale>
    </locale-config>
    <message-bundle>oracle.bpel.services.workflow.worklist.resources.worklist</message-bundle>
    <resource-bundle>
      <base-name>oracle.bpel.services.workflow.worklist.resources.worklist</base-name>
      <var>resources</var>
    </resource-bundle>
  </application>
  <converter>
    <converter-for-class>oracle.bpel.services.datacontrol.types.Number</converter-for-class>
    <converter-class>oracle.bpel.services.datacontrol.types.CustomNumberConverter</converter-class>
  </converter>
  <converter>
    <converter-for-class>oracle.bpel.services.datacontrol.types.XSDate</converter-for-class>
    <converter-class>oracle.bpel.services.datacontrol.types.XSDateConverter</converter-class>
  </converter>
  <converter>
    <converter-for-class>oracle.bpel.services.datacontrol.types.XSTime</converter-for-class>
    <converter-class>oracle.bpel.services.datacontrol.types.XSTimeConverter</converter-class>
  </converter>
  <converter>
    <converter-for-class>oracle.bpel.services.datacontrol.types.XSDateTime</converter-for-class>
    <converter-class>oracle.bpel.services.datacontrol.types.XSDateTimeConverter</converter-class>
  </converter>
  <converter>
    <converter-id>dateToStringConverter</converter-id>
    <converter-class>oracle.bpel.services.datacontrol.types.DateToStringConverter</converter-class>
  </converter>
  <validator>
    <validator-id>ValidateSubPart</validator-id>
    <validator-class>gov.laca.amp.portal.validators.ValidateSubPart</validator-class>
  </validator>
  <validator>
    <validator-id>ValidateWholeNumbers</validator-id>
    <validator-class>gov.laca.amp.portal.validators.ValidateWholeNumbers</validator-class>
  </validator>
  <validator>
    <validator-id>ValidateQualityClass</validator-id>
    <validator-class>gov.laca.amp.portal.validators.ValidateQualityClass</validator-class>
  </validator>
  <validator>
    <validator-id>ValidateYearRange</validator-id>
    <validator-class>gov.laca.amp.portal.validators.ValidateYearRange</validator-class>
  </validator>
  <validator>
    <validator-id>ValidateDesignType</validator-id>
    <validator-class>gov.laca.amp.portal.validators.ValidateDesignType</validator-class>
  </validator>
  <validator>
    <validator-id>ValidateDepreciationTable</validator-id>
    <validator-class>gov.laca.amp.portal.validators.ValidateDepreciationTable</validator-class>
  </validator>
  <validator>
    <validator-id>ValidateDepreciationType</validator-id>
    <validator-class>gov.laca.amp.portal.validators.ValidateDepreciationType</validator-class>
  </validator>
  <validator>
    <validator-id>ValidateDecimal</validator-id>
    <validator-class>gov.laca.amp.portal.validators.ValidateDecimal</validator-class>
  </validator>
  <converter>
    <converter-id>AMPConvertNumber</converter-id>
    <converter-class>gov.laca.amp.portal.converters.AMPConvertNumber</converter-class>
  </converter>
  <converter>
    <converter-id>AMPConvertString</converter-id>
    <converter-class>gov.laca.amp.portal.converters.AMPConvertString</converter-class>
  </converter>
  <converter>
    <converter-id>AMPConvertCurrency</converter-id>
    <converter-class>gov.laca.amp.portal.converters.AMPConvertCurrency</converter-class>
  </converter>
</faces-config>